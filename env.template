# InstantDashboard Environment Configuration Template
# Copy this file to .env and fill in your actual values

# Model Configuration
ROOT_AGENT_MODEL=gemini-2.0-flash-001
ANALYTICS_AGENT_MODEL=gemini-1.5-pro
BASELINE_NL2SQL_MODEL=gemini-1.5-pro
BIGQUERY_AGENT_MODEL=gemini-1.5-pro
BQML_AGENT_MODEL=gemini-1.5-pro
CHASE_NL2SQL_MODEL=gemini-1.5-pro

# Google Cloud Configuration
GOOGLE_CLOUD_PROJECT=your-project-id-here
GOOGLE_CLOUD_LOCATION=us-central1
GOOGLE_GENAI_USE_VERTEXAI=1
GOOGLE_CLOUD_STORAGE_BUCKET=your-bucket-name-here

# BigQuery Configuration
BQ_PROJECT_ID=your-bigquery-project-id
BQ_DATASET_ID=your-dataset-name

# BQML and RAG Configuration (optional)
BQML_RAG_CORPUS_NAME=your-corpus-name-here
CODE_INTERPRETER_EXTENSION_NAME=your-extension-name

# NL2SQL Method (BASELINE or CHASE)
NL2SQL_METHOD=BASELINE

# PostgreSQL Database Configuration (removed - not needed for InstantDashboard)
# Note: InstantDashboard uses BigQuery, no PostgreSQL needed

# Google OAuth Configuration (for user authentication)
GOOGLE_OAUTH_CLIENT_ID=your-google-oauth-client-id
GOOGLE_OAUTH_CLIENT_SECRET=your-google-oauth-client-secret
GOOGLE_OAUTH_REDIRECT_URI=http://localhost:8001/auth/callback

# Session and Security
SECRET_KEY=your-secret-key-for-jwt-tokens
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30 